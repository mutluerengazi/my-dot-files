{
  "base_keymap": "VSCode",
  "theme": "Catppuccin Macchiato",
  "vim_mode": true,
  //font
  "ui_font_size": 13,
  "buffer_font_size": 12,
  "ui_font_family": "Zed Plex Sans",
  "buffer_font_family": "Zed Plex Mono",
  "cursor_blink": false,
  //other
  "git": {
    "inline_blame": {
      "enabled": false
    }
  },
  "hour_format": "hour24",
  "relative_line_numbers": true,
  "project_panel": {
    "button": true,
    "dock": "right",
    "git_status": true
  },
  "terminal": {
    "font_family": "JetBrainsMono Nerd Font",
    "blinking": "off"
  }
}
